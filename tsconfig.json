{
    "compilerOptions": {
        "jsx": "react", 
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        },
        "module": "commonjs",
        "target": "es6", 
        "lib": ["dom", "es2015", "es2017"],
        "strict": false,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false
    }
}